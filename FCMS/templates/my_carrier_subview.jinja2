                      </div>
                      <!-- /.user-block -->
                      <p>
                                    Salary:
                                    {{ val.crewMember.salary }}<br>
                                    Enabled:
                                    {{ val.crewMember.enabled }}<br>
                      </p>

                      <p>
                          Status: {{ val.status }}
                        <span class="float-right">
                            <i class="fas fa-cog mr-1"></i> Last change: {{ val.crewMember.lastEdit }}
                        </span>
                      </p>
